{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "jquery",
    "ecma5",
    "ecma6",
    "underscore"
  ],
  "loadEagerly": [
    "/node_modules/react/dist/*.js",
    "/node_modules/react-dom/dist/*.js",
    "/node_modules/react-relay/dist/*.js",
    "/node_modules/react-static-container/lib/*.js"
  ],
  "dontLoad": [
    "path/to/your/js/**/*.js"
  ],
  "plugins": {
    "complete_strings": {
      "maxLength": 15
    },
    "node": {},
    "es_modules": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}
